    "is-mobile": "^0.2.2",
    "is-safari": "^1.0.0",
    "lodash.range": "^3.1.0",
    "mesh-reindex": "^1.0.0",
    "normals": "^1.0.1",
    "right-now": "^1.0.0",
    "tap-spec": "^4.1.1",
